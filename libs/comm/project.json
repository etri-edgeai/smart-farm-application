{
  "name": "comm",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/comm/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/comm/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/comm"],
      "options": {
        "tsConfig": "libs/comm/tsconfig.lib.json",
        "jestConfig": "libs/comm/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
