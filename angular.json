{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": false,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build",
            "args": ["resources/config/common.conf"]
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "config": {
      "root": "libs/config",
      "sourceRoot": "libs/config/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/config/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/config"],
          "options": {
            "jestConfig": "libs/config/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "db": {
      "root": "libs/db",
      "sourceRoot": "libs/db/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/db/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/db"],
          "options": {
            "jestConfig": "libs/db/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "edge-mon": {
      "projectType": "application",
      "root": "apps/edge-mon",
      "sourceRoot": "apps/edge-mon/src",
      "prefix": "smart-farm-prediction",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/frontend/edge-mon",
            "index": "apps/edge-mon/src/index.html",
            "main": "apps/edge-mon/src/main.ts",
            "polyfills": "apps/edge-mon/src/polyfills.ts",
            "tsConfig": "apps/edge-mon/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/edge-mon/src/favicon.ico",
              "apps/edge-mon/src/assets"
            ],
            "styles": ["apps/edge-mon/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/edge-mon/src/environments/environment.ts",
                  "with": "apps/edge-mon/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "edge-mon:build:production"
            },
            "development": {
              "browserTarget": "edge-mon:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/edge-mon/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "edge-mon:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/edge-mon/src/**/*.ts",
              "apps/edge-mon/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/edge-mon"],
          "options": {
            "jestConfig": "apps/edge-mon/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "edge-mon-e2e": {
      "root": "apps/edge-mon-e2e",
      "sourceRoot": "apps/edge-mon-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/edge-mon-e2e/cypress.json",
            "devServerTarget": "edge-mon:serve:development",
            "tsConfig": "apps/edge-mon-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "edge-mon:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/edge-mon-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["edge-mon"]
    },
    "ml": {
      "projectType": "application",
      "root": "apps/ml",
      "sourceRoot": "apps/ml/src",
      "architect": {
        "build": {
          "builder": "@nx-python/nx-python:build",
          "options": {
            "outputPath": "dist/ml",
            "main": "apps/ml/src/main.py",
            "templateType": "default"
          }
        },
        "serve": {
          "builder": "@nx-python/nx-python:serve",
          "options": {
            "main": "apps/ml/src/main.py",
            "templateType": "default"
          }
        },
        "test": {
          "builder": "@nx-python/nx-python:test",
          "options": {
            "main": "apps/ml/src/test_hello.py",
            "templateType": "default"
          }
        },
        "lint": {
          "builder": "@nx-python/nx-python:lint",
          "options": {
            "main": "apps/ml/src/main.py",
            "templateType": "default"
          }
        }
      },
      "tags": []
    },
    "ml-serving": {
      "projectType": "application",
      "root": "apps/ml-serving",
      "sourceRoot": "apps/ml-serving/src",
      "architect": {
        "build": {
          "builder": "@nx-python/nx-python:build",
          "options": {
            "outputPath": "dist/apps/ml-serving",
            "main": "apps/ml-serving/src/hello.py",
            "templateType": "default"
          }
        },
        "serve": {
          "builder": "@nx-python/nx-python:serve",
          "options": {
            "main": "apps/ml-serving/src/hello.py",
            "templateType": "default"
          }
        },
        "test": {
          "builder": "@nx-python/nx-python:test",
          "options": {
            "main": "apps/ml-serving/src/test_hello.py",
            "templateType": "default"
          }
        },
        "lint": {
          "builder": "@nx-python/nx-python:lint",
          "options": {
            "main": "apps/ml-serving/src/hello.py",
            "templateType": "default"
          }
        }
      },
      "tags": []
    },
    "models": {
      "root": "libs/models",
      "sourceRoot": "libs/models/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/models/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/models"],
          "options": {
            "jestConfig": "libs/models/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "sensor": {
      "root": "apps/sensor",
      "sourceRoot": "apps/sensor/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/sensor",
            "main": "apps/sensor/src/main.ts",
            "tsConfig": "apps/sensor/tsconfig.app.json",
            "assets": ["apps/sensor/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/sensor/src/environments/environment.ts",
                  "with": "apps/sensor/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "sensor:build",
            "port": 9228,
            "args": ["resources/config/common.conf"]
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/sensor/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/sensor"],
          "options": {
            "jestConfig": "apps/sensor/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
  }
}
