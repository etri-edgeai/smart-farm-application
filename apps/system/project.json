{
  "name": "system",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/system/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/system",
        "main": "apps/system/src/main.ts",
        "tsConfig": "apps/system/tsconfig.app.json",
        "assets": ["apps/system/src/assets"],
        "webpackConfig": "apps/system/webpack.config.js",
        "isolatedConfig": true,
        "babelUpwardRootMode": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/system/src/environments/environment.ts",
              "with": "apps/system/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "system:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "system:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/system/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/system/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
