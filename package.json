    "postinstall:dev": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve web --host 0.0.0.0",
    "serve:ml": "cd devenv && PYTHONPATH=$PWD/../apps python ../apps/ml/main.py",
