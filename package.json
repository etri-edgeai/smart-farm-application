  "scripts": {
    "ng": "nx",
    "postinstall:dev": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "serve:ml": "cd devenv && PYTHONPATH=$PWD/../apps python ../apps/ml/main.py",
    "build": "nx build",
    "build:all": "nx build api --prod & nx build sensor-processor --prod & nx build ai --prod & nx build edge-mon --prod",
    "test": "nx test",
    "devenv": "mkdir -p devenv && cp -nR ./resources/config devenv ",
    "copyresources": "cp -R ./resources/config ./resources/services dist/",
    "copydist": "sudo cp -R ./dist/frontend/ ./dist/api/ ./dist/sensor-forward/ ./apps/ai ./apps/ai_train/ ./devenv/ai_models/ /farmconnect/"
  },
