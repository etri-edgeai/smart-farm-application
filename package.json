  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "serve:ml": "cd devenv && PYTHONPATH=$PWD/../apps python ../apps/ml/main.py",
    "build": "nx build",
    "build:all": "nx build api --prod & nx build sensor --prod & nx build farm --prod & nx build edge-mon --prod",
    "test": "nx test",
    "devenv": "mkdir -p devenv && cp -nR ./resources/config devenv ",
    "copyresources": "cp -R ./resources/config ./resources/services dist/",
    "copydist": "sudo cp -R ./dist/frontend/ ./dist/api/ ./dist/sensor/ ./dist/farm/ ./apps/ml ./apps/ml_common/ ./apps/ml_center/ ./devenv/ml_models/ /farmconnect/"
  },
